.container
  = form_with(model: article, local: true) do |f|
    %div
      = f.label :eyecatch, 'New Board', class: 'top-titile'
    %div
      -# = f.file_field :eyecatch
    %div
      = f.label :title, 'Name'
    %div
      = f.text_field :title, class: 'text'
    %div
      = f.label :content, 'Description'
    %div
      = f.text_area :content
    = f.submit 'submit', class: 'btn'
    
